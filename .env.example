# Supabase Configuration
# Get these from your Supabase project settings
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE=your_supabase_service_role_key

# CSRF Protection (generate a new random key for each environment)
CSRF_SECRET=generate_a_random_32_character_string

# Encryption Key (generate a new random key for each environment)
ENCRYPTION_KEY=generate_a_random_32_character_string

# Disable Ollama for development
DISABLE_OLLAMA=true

# AI Model API Keys (get your own keys from respective providers)
OPENAI_API_KEY=your_openai_api_key
MISTRAL_API_KEY=your_mistral_api_key
GOOGLE_GENERATIVE_AI_API_KEY=your_gemini_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
XAI_API_KEY=your_xai_api_key
PERPLEXITY_API_KEY=your_perplexity_api_key
OPENROUTER_API_KEY=your_openrouter_api_key

# Ollama Configuration (for local AI models)
OLLAMA_BASE_URL=http://localhost:11434

# Developer Tools (optional)
EXA_API_KEY=your_exa_api_key
GITHUB_TOKEN=your_github_token

# Polar Payment Configuration (get your own from Polar)
POLAR_ACCESS_TOKEN=your_polar_access_token
POLAR_WEBHOOK_SECRET=your_polar_webhook_secret
POLAR_PRO_PRODUCT_ID=your_polar_pro_product_id
POLAR_PRO_PRICE_ID=your_polar_pro_price_id
POLAR_UNLIMITED_PRODUCT_ID=your_polar_unlimited_product_id
POLAR_UNLIMITED_PRICE_ID=your_polar_unlimited_price_id

# Production Configuration (optional)
NEXT_PUBLIC_VERCEL_URL=your_production_domain

# Development Tools (optional)
ANALYZE=false